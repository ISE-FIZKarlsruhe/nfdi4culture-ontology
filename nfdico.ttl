@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix ovm: <http://omv.ontoware.org/2005/05/ontology#> .
@prefix bibo: <http://purl.org/ontology/bibo/> .
@prefix sd: <https://w3id.org/okn/o/sd#> .

nfdico:
  a owl:Ontology ;
  dc:title "NFDI4Culture Ontology" ;
  rdfs:seeAlso <https://nfdi4culture.de/ontology/about.html>, <https://nfdi4culture.de/ontology.rdf>, <https://nfdi4culture.de/ontology.ttl>, <https://nfdi4culture.de/ontology.nt>, <https://nfdi4culture.de/ontology.json> ;
  rdfs:label "NFDI4Culture Ontology (nfdico)" ;
  dc:creator n4c:E2415, n4c:E2416, n4c:E1925, n4c:E3095 ;
  dcterms:abstract "The NFDICO core ontology describes all resources (datasets, data providers, persons, projects and other entities) in the data domain of NFDI4Culture (see: https:\\/\\/nfdi4culture.de\\/resource\\/about.html). It serves as the basis for the NFDI4Culture Knowledge Graph (see: https:\\/\\/nfdi4culture.de\\/resources\\/knowledge-graph.html) and contains mappings to numerous external vocabularies and ontologies."@en ;
  dcterms:hasVersion "1.1" .

<https://nfdi4culture.de/ontology/about.html>
  a void:Dataset ;
  void:feature <http://www.w3.org/ns/formats/RDFa> ;
  dc:format "text/html" .

<https://nfdi4culture.de/ontology.rdf>
  a void:Dataset ;
  void:feature <http://www.w3.org/ns/formats/RDF_XML> ;
  dc:format "application/rdf+xml" .

<https://nfdi4culture.de/ontology.ttl>
  a void:Dataset ;
  void:feature <http://www.w3.org/ns/formats/Turtle> ;
  dc:format "text/turtle" .

<https://nfdi4culture.de/ontology.nt>
  a void:Dataset ;
  void:feature <http://www.w3.org/ns/formats/N-Triples> ;
  dc:format "application/n-triples" .

<https://nfdi4culture.de/ontology.json>
  a void:Dataset ;
  void:feature <http://www.w3.org/ns/formats/JSON-LD> ;
  dc:format "application/ld+json" .

nfdico:AcademicDiscipline
  a owl:Class ;
  rdfs:label "academic discipline"@en ;
  rdfs:comment "An academic discipline, a research field or domain of a resource, e.g. a data portal."@en ;
  rdfs:subClassOf fabio:SubjectDiscipline, modsci:Science, dbo:AcademicSubject ;
  nfdico:example "Architecture, Art History, Musicology, Media Studies, Performing Arts"@en, wikidata:Q12271 .

nfdico:Authority
  a owl:Class ;
  rdfs:label "authority"@en ;
  rdfs:comment "A collection of selected decisions or rules for a form of entry that result in the consistency of entries to improve interoperability and retrieval of data. "@en ;
  rdfs:subClassOf schema:Product ;
  nfdico:example "Art & Architecture Thesaurus, Gemeinsame Norm Datei, Cultural Objects Names Authority"@en .

nfdico:AuthorityType
  a owl:Class ;
  rdfs:label "authority type"@en ;
  rdfs:comment "The type of an authority used for the establishment and maintainance of consistent forms of terms."@en ;
  nfdico:example "Thesaurus, Authority File, Authority List"@en .

nfdico:BestPractice
  a owl:Class ;
  rdfs:label "best practice"@en ;
  rdfs:comment "A resource that identifies a standard or preferred procedure within a specific domain."@en ;
  rdfs:subClassOf schema:Product ;
  nfdico:example "Persistent Identifier, DMB standards"@en .

nfdico:City
  a owl:Class ;
  rdfs:label "city"@en ;
  rdfs:comment "A city or a town where the thing is located."@en ;
  nfdico:example "Berlin, Hannover, Kyiv"@en .

nfdico:CollectionOrDataset
  a owl:Class ;
  rdfs:label "collection or dataset"@en ;
  rdfs:comment "A collection of data published or curated by an organization or a peron."@en ;
  rdfs:subClassOf nfdico:Contribution ;
  nfdico:example "RISM catalogue of musical sources"@en .

nfdico:Contribution
  a owl:Class ;
  rdfs:label "contribution"@en ;
  rdfs:comment "A contribution to a project made by an organization or person. Subclasses are used to specify particular contributions, e.g. DataPortal or Software."@en ;
  rdfs:subClassOf schema:Product .

nfdico:ControlledVocabulary
  a owl:Class ;
  rdfs:label "controlled vaocabulary"@en ;
  rdfs:comment "A collection of controlled predefined vocabularies used in a specific domain to organize the knowledge."@en ;
  rdfs:subClassOf nfdico:Contribution ;
  nfdico:example "AAT (Art & Architecture Thesaurus), Schema.org" .

nfdico:Country
  a owl:Class ;
  rdfs:label "country"@en ;
  rdfs:comment "A country where the thing is located."@en ;
  rdfs:subClassOf nfdico:Place .

nfdico:DataPortal
  a owl:Class ;
  rdfs:label "data portal"@en ;
  rdfs:comment "An online platform that provides access to data collections and datasets."@en ;
  rdfs:subClassOf nfdico:Contribution ;
  nfdico:example "Bach digital, KultSam, MusiXplora"@en .

nfdico:DevelopmentWork
  a owl:Class ;
  rdfs:label "development work"@en ;
  rdfs:comment "A resource that identifies an ongoing development of standards, services, data models, vocabularies, ontologies, etc."@en ;
  rdfs:subClassOf nfdico:Contribution ;
  nfdico:example "Development of a controlled vocabulary for audivisual material in publication and research data; Extension of the AAT vocabulary for the history of architecture"@en .

nfdico:FederalState
  a owl:Class ;
  rdfs:label "federal state"@en ;
  rdfs:comment "A federal state where the thing is located."@en ;
  rdfs:subClassOf nfdico:Place ;
  nfdico:example "Sachsen, Berlin"@de .

nfdico:InstanceOfRDMTool
  a owl:Class ;
  rdfs:label "instance of research data management tool"@en ;
  rdfs:comment "A resource that identifies a single copy of an RDM Tool running on a single physical or virtual server."@en ;
  rdfs:subClassOf schema:Product ;
  nfdico:example "Research Data management Organiser, EPrints"@en .

nfdico:License
  a owl:Class ;
  rdfs:label "license"@en ;
  rdfs:comment "A document that provides legal guidelines for the use of a resource, e.g. a software."@en ;
  nfdico:example "cc-by-3, Digital Peer Publishing Lizenz"@en ;
  rdfs:subClassOf vivo:Licence, dcterms:LicenseDocument, swo:SWO_0000002 .

nfdico:Mapping
  a owl:Class ;
  rdfs:label "mapping"@en ;
  rdfs:comment "A resource that documents a mapping from a format\\/vocabulary to another format\\/vocabulary."@en ;
  nfdico:example "Bach Digital Mapping, WeGA Gudielines"@en .

nfdico:MediaType
  a owl:Class ;
  rdfs:label "media type"@en ;
  rdfs:comment "A format or a physical medium of digital data."@en ;
  rdfs:subClassOf dcterms:FileFormat ;
  nfdico:example "doc, avi, epub" .

nfdico:Norm
  a owl:Class ;
  rdfs:label "norm"@en ;
  rdfs:comment "An international standard and norm that can be used to evaluate or create services."@en ;
  rdfs:subClassOf schema:Product ;
  nfdico:example "ISO 15511, DINI-Zertifikat" .

nfdico:ObjectType
  a owl:Class ;
  rdfs:label "object type"@en ;
  rdfs:comment "A resource that identifies the type of object in a data collection."@en ;
  nfdico:example "book, photograph, music sheet"@en .

nfdico:Ontology
  a owl:Class ;
  rdfs:label "ontology"@en ;
  rdfs:comment "An ontology used for formal representations of concepts in a resource, e.g. in data collection."@en ;
  rdfs:subClassOf schema:Product ;
  nfdico:example "lido, gndo, dbo, dublin-core" .

nfdico:OntologyType
  a owl:Class ;
  rdfs:label "ontology type"@en ;
  rdfs:comment "A resource that identifies the type of an ontology."@en ;
  nfdico:example "encoding\\/vocabulary, ontology, controlled vocabulary"@en .

nfdico:Organization
  a owl:Class ;
  rdfs:label "organization"@en ;
  rdfs:comment "An organization of some kind, e.g. research organization or business organization"@en ;
  nfdico:example "Universit\\u00e4tsbibliothek Leipzig, S\\u00e4chsisches Wirtschaftsarchiv, Museen der Stadt K\\u00f6ln" .

nfdico:Person
  a owl:Class ;
  rdfs:label "person"@en ;
  rdfs:comment "A resource that represents a single person."@en ;
  nfdico:example "Harald Sack, Tabea Tietz, Sasha Bruns, Etienne Posthumus" .

nfdico:Place
  a owl:Class ;
  rdfs:label "place"@en ;
  rdfs:comment "A spatial region, e.g. country, city or district."@en ;
  rdfs:subClassOf vivo:Location, obo:GAZ_00000448, vcard:Location, dcterms:Location ;
  nfdico:example "Hessen, Berlin, Schweiz" .

nfdico:ProgrammingLanguage
  a owl:Class ;
  rdfs:label "programming language"@en ;
  rdfs:comment "A formal language used for implementing a software."@en ;
  rdfs:subClassOf schema:ComputerLanguage ;
  nfdico:example "Java, Ruby, C++, Perl, PHP" .

nfdico:Project
  a owl:Class ;
  rdfs:label "project"@en ;
  rdfs:comment "A scientific or business endeavor that aims to conclude an investigation or to answer a research question."@en ;
  rdfs:subClassOf foaf:Project, schema:Project, doap:Project ;
  owl:equivalentClass vivo:ResearchProject ;
  nfdico:example "Freisch\\u00fctz digital, Corpus Vitrearum Medii Aevi" .

nfdico:Publication
  a owl:Class ;
  rdfs:label "publication"@en ;
  rdfs:comment "A published scholarly work that reports on ongoing activity about or within a resource, e.g. a dataset."@en ;
  rdfs:subClassOf dcterms:BibliographicResource, fabio:ScholarlyWork ;
  nfdico:example "MusiXplora: Visual analysis of a musicological encyclopedia, Textkategorien in kompositorischen Werkstattdokumenten" .

nfdico:RDMInfrastructure
  a owl:Class ;
  rdfs:label "research data management infrastructure"@en ;
  rdfs:comment "A infrastructure used to provide research data management and enforce research data management policies."@en ;
  rdfs:subClassOf schema:Product ;
  nfdico:example "correspSearch, Bildindex Kunst und Architektur"@en .

nfdico:RDMService
  a owl:Class ;
  rdfs:label "research data management service"@en ;
  rdfs:comment "A service that aims at providing support at every step of research data management, e.g. investigation, development , sustainbility of research data."@en ;
  rdfs:subClassOf nfdico:RDMInfrastructure ;
  nfdico:example "Untersuchung und Entwicklung von bildbasierten Such- und Analyseverfahren, Nachhaltigkeit f\\u00fcr Forschungssoftware"@de .

nfdico:RDMTool
  a owl:Class ;
  rdfs:label "research data management tool"@en ;
  rdfs:comment "A software tool used for tackling a specific research data management problem."@en ;
  rdfs:subClassOf nfdico:RDMInfrastructure ;
  nfdico:example "Annotationswerkzeug f\\u00fcr ethnographisch erhobene Daten der Musikwissenschaft"@de .

nfdico:Service
  a owl:Class ;
  rdfs:label "service"@en ;
  rdfs:comment "A service, expertise or skill provided by an organization or a person."@en ;
  rdfs:subClassOf frapo:Service, obo:ERO_0000005, dcmitype:Service, schema:Service, nfdico:Contribution ;
  nfdico:example "Data curation for HeiDATA, Expertise for the development and application of LIDO"@en .

nfdico:ServiceType
  a owl:Class ;
  rdfs:label "service type"@en ;
  rdfs:comment "A resource that identifies the type of a service."@en ;
  nfdico:example "Expertise, advertisement, curation"@en .

nfdico:Software
  a owl:Class ;
  rdfs:label "software"@en ;
  rdfs:comment "A computer software provided by an organization or a person."@en ;
  rdfs:subClassOf nfdico:Contribution ;
  nfdico:example "Edirom, Muscat" .

nfdico:SparqlEndpoint
  a owl:Class ;
  rdfs:label "sparql endpoint"@en ;
  rdfs:comment "An endpoint URL."@en .

nfdico:Specification
  a owl:Class ;
  rdfs:label "specification"@en ;
  rdfs:comment "A set of requirements, guidelines, standards contributed by an organization or a person."@en ;
  rdfs:subClassOf nfdico:Contribution ;
  nfdico:example "RDF, SPARQL, Federation" .

nfdico:Technology
  a owl:Class ;
  rdfs:label "technology"@en ;
  rdfs:comment "A technology, e.g. a web application or a programming framework used in a resource."@en ;
  rdfs:subClassOf schema:Product ;
  nfdico:example "Groovy on Grails, Angular"@en .

nfdico:Website
  a owl:Class ;
  rdfs:label "website"@en ;
  rdfs:comment "A collection of webpages contributed by an organization or a person."@en ;
  rdfs:subClassOf foaf:Document, nfdico:Contribution ;
  nfdico:example "https:\\/\\/opac.rism.info" .

nfdico:acronym
  a rdf:Property, owl:AnnotationProperty ;
  rdfs:label "nfdico:acronym"@en .

nfdico:authorityControl
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "authority control"@en ;
  rdfs:comment "The property is a relationship between data and an authority data is based on."@en ;
  owl:inverseOf nfdico:authorityOf ;
  rdfs:domain nfdico:Contribution ;
  rdfs:range nfdico:Authority .

nfdico:authorityOf
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "authority of"@en .

nfdico:authorityType
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "authority type"@en ;
  rdfs:comment "The property is a relationship between an authority resource and its type."@en ;
  owl:inverseOf nfdico:authorityTypeOf ;
  rdfs:domain nfdico:Authority ;
  rdfs:range nfdico:AuthorityType .

nfdico:authorityTypeOf
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "authority type of"@en .

nfdico:bestPractice
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "best practice"@en ;
  rdfs:comment "The property is a relationship between a data portal and the best practice."@en ;
  owl:inverseOf nfdico:bestPracticeOf ;
  rdfs:domain nfdico:Contribution ;
  rdfs:range nfdico:BestPractice .

nfdico:bestPracticeOf
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "best practice of"@en .

nfdico:contactPoint
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "contact point"@en ;
  rdfs:comment "The property is a relationship between a contribution and a contact point."@en ;
  owl:inverseOf nfdico:contactPointOf ;
  rdfs:domain nfdico:Contribution ;
  rdfs:range [
    a owl:Class ;
    owl:unionOf (
     nfdico:Organization
     nfdico:Person
   )
  ] .

nfdico:contactPointOf
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "contact point of"@en .

nfdico:contributor
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "contributor"@en ;
  rdfs:comment "The property is a relationship between a contribution and the organization that has contributed it. "@en ;
  owl:inverseOf nfdico:contributorOf ;
  rdfs:subPropertyOf dcterms:contributor ;
  rdfs:domain nfdico:Contribution ;
  rdfs:range [
    a owl:Class ;
    owl:unionOf (
     nfdico:Organization
     nfdico:Person
   )
  ] .

nfdico:contributorOf
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "contributor of"@en .

nfdico:country
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "country"@en ;
  rdfs:comment "The property relates a city or a state to a country where it is located."@en ;
  rdfs:subPropertyOf dbo:country ;
  owl:inverseOf nfdico:countryOf ;
  rdfs:domain [
    a owl:Class ;
    owl:unionOf (
     nfdico:City
     nfdico:FederalState
   )
  ] ;
  rdfs:range nfdico:Country .

nfdico:countryOf
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "country of"@en .

nfdico:example
  a rdf:Property, owl:AnnotationProperty ;
  rdfs:label "nfdico:example"@en .

nfdico:exportFormat
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "export format"@en ;
  rdfs:comment "The property relates a contribution to the format available for the export of a contribution."@en ;
  owl:inverseOf nfdico:exportFormatOf ;
  rdfs:subPropertyOf dcterms:format ;
  rdfs:domain nfdico:Contribution ;
  rdfs:range nfdico:MediaType .

nfdico:exportFormatOf
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "export format of"@en .

nfdico:federalState
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "federal state"@en ;
  rdfs:comment "The property relates a city to a federal state where it is located."@en ;
  owl:inverseOf nfdico:federalStateOf ;
  rdfs:domain nfdico:City ;
  rdfs:range nfdico:FederalState .

nfdico:federalStateOf
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "federal state of"@en .

nfdico:funder
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "funder"@en ;
  rdfs:comment "The property relates thing to an organization or a project that provided funding for it. The subproperties are used to specify more specific funding relationships. "@en ;
  rdfs:subPropertyOf foaf:fundedBy, schema:funder ;
  rdfs:domain owl:Thing ;
  rdfs:range [
    a owl:Class ;
    owl:unionOf (
     nfdico:Organization
     nfdico:Project
   )
  ] .

nfdico:funderOf
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "funder of"@en .

nfdico:fundingOrganization
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "funding organization"@en ;
  rdfs:comment "The property relates a contribution or a project to the organization that provided funding for it. "@en ;
  rdfs:subPropertyOf frapo:hasFundingAgency, nfdico:funder ;
  owl:inverseOf nfdico:fundingOrganizationOf ;
  rdfs:domain [
    a owl:Class ;
    owl:unionOf (
     nfdico:Contribution
     nfdico:Project
   )
  ] ;
  rdfs:range nfdico:Organization .

nfdico:fundingOrganizationOf
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "funding organization of"@en ;
  rdfs:subPropertyOf nfdico:funderOf .

nfdico:fundingProject
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "funding project"@en ;
  rdfs:comment "The property relates a contribution to the project that provided funding for it."@en ;
  owl:inverseOf nfdico:fundingProjectOf ;
  rdfs:subPropertyOf nfdico:funder ;
  rdfs:domain nfdico:Contribution ;
  rdfs:range nfdico:Project .

nfdico:fundingProjectOf
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "funding project of"@en ;
  rdfs:subPropertyOf nfdico:funderOf .

nfdico:url
  a rdf:Property, owl:DatatypeProperty ;
  rdfs:label "url"@en ;
  rdfs:comment "URL of a thing."@en ;
  rdfs:domain owl:Thing ;
  rdfs:range xsd:anyURI .

nfdico:instance
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "instance"@en ;
  rdfs:comment "The property is a relationship between an RDM Tool to its running instance."@en ;
  owl:inverseOf nfdico:instanceOf ;
  rdfs:domain nfdico:RDMTool ;
  rdfs:range nfdico:InstanceOfRDMTool .

nfdico:instanceOf
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "instance of"@en .

nfdico:license
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "license"@en ;
  rdfs:comment "The property is a relationship between a contribution and a license file." ;
  owl:inverseOf nfdico:licenseOf ;
  rdfs:subPropertyOf dcterms:license ;
  rdfs:domain [
    a owl:Class ;
    owl:unionOf (
     nfdico:Contribution
   )
  ] ;
  rdfs:range nfdico:License .

nfdico:licenseOf
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "license of"@en .

nfdico:location
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "location"@en ;
  rdfs:comment "The property is a relationship between a thing and a place its located at. "@en ;
  owl:inverseOf nfdico:locationOf ;
  rdfs:domain owl:Thing ;
  rdfs:range nfdico:Place .

nfdico:locationOf
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "location of"@en .

nfdico:mapping
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "mapping"@en ;
  rdfs:comment "The property is a relationship between a data resource and its various representations, e.g. same resource that exists in different data formats. "@en ;
  rdfs:subPropertyOf dk:mapsTo ;
  owl:inverseOf nfdico:mappingOf ;
  rdfs:domain nfdico:Contribution ;
  rdfs:range nfdico:Mapping .

nfdico:mappingOf
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "mapping of"@en .

nfdico:mediaType
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "media type"@en ;
  rdfs:comment "The property relates a contribution to its data format."@en ;
  rdfs:subPropertyOf dcterms:format ;
  owl:inverseOf nfdico:mediaTypeOf ;
  rdfs:domain nfdico:Contribution ;
  rdfs:range nfdico:MediaType .

nfdico:mediaTypeOf
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "media type of"@en .

nfdico:norm
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "norm"@en ;
  rdfs:comment "The property is a relationship between a resource and the international standard or norm."@en ;
  rdfs:subPropertyOf dcterms:conformsTo ;
  owl:inverseOf nfdico:normOf ;
  rdfs:domain [
    a owl:Class ;
    owl:unionOf (
     nfdico:BestPractice
     nfdico:DevelopmentWork
     nfdico:RDMInfrastructure
     nfdico:Service
   )
  ] ;
  rdfs:range nfdico:Norm .

nfdico:normOf
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "norm of"@en .

nfdico:objectType
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "object type"@en ;
  rdfs:comment "The property relates a data collection to the type of object it contains. "@en ;
  owl:inverseOf nfdico:objectTypeOf ;
  rdfs:domain nfdico:CollectionOrDataset ;
  rdfs:range nfdico:ObjectType .

nfdico:objectTypeOf
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "object type of"@en .

nfdico:ontologyType
  a rdf:Property, owl:ObjectProperty ;
  rdfs:label "ontology type"@en ;
  rdfs:comment "The property relates an ontology to its type. "@en ;
  owl:inverseOf nfdico:ontologyTypeOf ;
  rdfs:domain nfdico:Ontology ;
  rdfs:range nfdico:OntologyType .

nfdico:ontologyTypeOf
  a rdf:Property, owl:ObjectProperty ;
